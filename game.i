 if ((~((*(volatile unsigned short *)0x04000130)) & ((1<<4)))) {
  if (player.col < 240 - player.width && collisionMapBitmap[((player.row + (player.height / 2))*(240)+(player.col + player.width + 1))]
            && collisionMapBitmap[((player.row + player.height - 1)*(240)+(player.col + player.width + 1))]) {
   player.col += player.cdel;
  }
  player.aniState = PLAYERRIGHT;
